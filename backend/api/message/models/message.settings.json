{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "name": "Message"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "body": {
      "type": "richtext"
    },
    "published": {
      "type": "datetime"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "week": {
      "via": "messages",
      "model": "week"
    }
  }
}
